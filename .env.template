# Environment Variables Template
# Copy this file to .env and fill in your actual values

# API Keys
GOOGLE_API_KEY=your_google_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
BRAVE_API_KEY=your_brave_search_api_key_here
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here
OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud Configuration
GOOGLE_APPLICATION_CREDENTIALS=path_to_your_service_account_json
PROJECT_ID=your_project_id_here

# App Configuration
FLASK_ENV=production
PORT=8003

# Browser Use Configuration
BROWSER_USE_API_KEY=your_browser_use_api_key_here

# Note: Browser automation will use the system's default browser automatically
# No need to specify browser executable paths or user data directories